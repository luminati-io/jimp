{
  "name": "@luminati-io/plugin-print",
  "version": "0.16.1-lum.2",
  "description": "print an image.",
  "main": "dist/index.js",
  "module": "es/index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "cross-env BABEL_ENV=test mocha --require @babel/register",
    "test:watch": "npm run test -- --reporter min --watch",
    "test:coverage": "nyc npm run test",
    "build": "npm run build:node:production && npm run build:module",
    "build:watch": "npm run build:node:debug -- -- --watch --verbose",
    "build:debug": "npm run build:node:debug",
    "build:module": "cross-env BABEL_ENV=module babel src -d es --source-maps --config-file ../../babel.config.js",
    "build:node": "babel src -d dist --source-maps --config-file ../../babel.config.js",
    "build:node:debug": "cross-env BABEL_ENV=development npm run build:node",
    "build:node:production": "cross-env BABEL_ENV=production npm run build:node"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.7.2",
    "@luminati-io/utils": "link:../utils",
    "load-bmfont": "^1.4.0"
  },
  "peerDependencies": {
    "@luminati-io/custom": ">=0.3.5",
    "@luminati-io/plugin-blit": ">=0.3.5"
  },
  "devDependencies": {
    "@luminati-io/custom": "link:../custom",
    "@luminati-io/plugin-blit": "link:../plugin-blit",
    "@luminati-io/test-utils": "link:../test-utils"
  },
  "publishConfig": {
    "access": "public"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luminati-io/jimp.git"
  },
  "bugs": {
    "url": "https://github.com/luminati-io/jimp/issues"
  },
  "homepage": "https://github.com/luminati-io/jimp#readme"
}
